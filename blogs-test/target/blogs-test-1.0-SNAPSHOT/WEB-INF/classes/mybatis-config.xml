<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--
    配置数据源，交给Spring去做
    两件事：
        typeAliases
        mapper配置文件
    -->

    <typeAliases>
        <package name="org.su.pojo"/>
    </typeAliases>
    <mappers>
        <package name="org.su.dao"/>
    </mappers>

</configuration>